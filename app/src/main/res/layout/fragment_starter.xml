<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/onlysoon"
    android:fitsSystemWindows="true"
    tools:context=".Starter">




    <TextView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="ASpen"
    android:textSize="116sp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_marginTop="117dp"
    android:textColor="#fff"
        android:gravity="center"
        android:fontFamily="@font/histus"/>

    <TextView
        android:id="@+id/m4text"
        android:layout_width="244dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="11dp"
        android:text="Plan your"
        android:textColor="#fff"
        android:textSize="30sp"

        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@id/m4text1"
        app:layout_constraintBottom_toTopOf="@id/m4text1"/>

    <TextView
        android:id="@+id/m4text1"
        android:layout_width="344dp"
        android:layout_height="106dp"
        android:text="Luxurious Vacation"
        android:textColor="#fff"
        android:textSize="45sp"
        app:layout_constraintBottom_toTopOf="@+id/Explore"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginBottom="27dp"
        android:layout_marginStart="39dp"
        android:textStyle="normal"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/Explore"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="37dp"
        android:layout_marginBottom="57dp"
        android:background="@drawable/buttonshape"
        android:padding="8dp"
        android:text="Explore"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>